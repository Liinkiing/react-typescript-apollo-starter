{
  "name": "react-typescript-apollo-starter",
  "description": "A simple starter to work with GraphQL, React, Typescript and GraphQL code-gen for TS",
  "author": "Omar Jbara <omar.jbara2@gmail.com>",
  "version": "0.1.0",
  "repository": {
    "url": "https://github.com/Liinkiing/react-typescript-apollo-starter",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/Liinkiing/react-typescript-apollo-starter/issues"
  },
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "@types/graphql": "^14.2.3",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.2",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "cross-env": "^5.2.0",
    "graphql": "^14.5.0",
    "polished": "^3.4.1",
    "react": "^16.9.0",
    "react-apollo": "^3.0.1",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-hot-loader": "^4.12.11",
    "react-scripts": "3.1.1",
    "styled-components": "^4.3.2",
    "ts-optchain": "^0.1.8",
    "typescript": "3.5.3"
  },
  "scripts": {
    "gql-gen": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 gql-gen",
    "prestart": "yarn gql-gen",
    "start": "react-scripts start",
    "dev": "concurrently  \"react-scripts start\" \"yarn gql-gen --watch\" ",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "babelMacros": {
    "styledComponents": {
      "pure": true
    }
  },
  "devDependencies": {
    "@graphql-codegen/add": "^1.6.1",
    "@graphql-codegen/cli": "^1.6.1",
    "@graphql-codegen/fragment-matcher": "^1.6.1",
    "@graphql-codegen/typescript": "^1.6.1",
    "@graphql-codegen/typescript-operations": "^1.6.1",
    "@graphql-codegen/typescript-react-apollo": "^1.6.1",
    "@types/styled-components": "^4.1.18",
    "babel-plugin-styled-components": "^1.10.6"
  }
}
